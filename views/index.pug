extends layout

block content
  h1= title
  p Here you go... #{title}
  p Swarnalatha Yadla  Pug File 
  button(type='button', class = "btn btn-info" onclick="doBump()") Press Me
  <br />
  div(id="bump") Bump the count
  <br />
  script.
      var count = 0;
      var bump = 1;
      function doBump() {
          count+=bump;
          bump+=1;
          document.getElementById("bump").innerHTML=`Count ${count} with bump ${bump}`;
    }